    "outDir": "./bin/js/",
    "noImplicitAny": true,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "target": "es5",
    "module": "es6",
    "jsx": "react",
    "allowJs": true,
    "moduleResolution": "node",
    "baseUrl": ".",
    "paths": {
      "recharts": ["types/recharts/index.d.ts"]
    },
    "lib": [
      "dom",
      "es2015"
    ]
  },
  "file": [
    "./src_js/Index.tsx"
  ],
  "exclude": [
    "node_modules",
    "bin",
    "dist",
    "demo",
    "src_html"
  ]
}
